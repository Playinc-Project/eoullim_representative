# Staging example environment (테스트/스테이징용)
# 실제 스테이징 값으로 복사해서 사용하세요
DATABASE_HOST=eoullim-staging-db.example.com
DATABASE_PORT=3306
DATABASE_USER=eoullim
DATABASE_PASSWORD=changeme
DATABASE_NAME=eoullim_staging
DATABASE_SYNCHRONIZE=false

# Server
PORT=8081
NODE_ENV=development

# Enable TypeORM query logging when 필요 시 true로 설정
ENABLE_QUERY_LOGGING=true

# Logging
LOG_LEVEL=debug

# Timezone
TZ=Asia/Seoul
